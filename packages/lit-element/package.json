{
  "name": "@popeindustries/lit-element",
  "version": "3.1.4",
  "description": "Enable hydration of lit-element elements rendered on the server with @popeindustries/lit-html-server",
  "author": "Alexander Pope <alex@pope.industries>",
  "keywords": [
    "html template",
    "lit-html",
    "ssr",
    "stream",
    "streaming",
    "tagged template literal",
    "template",
    "template render"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "development": "./src/index.js",
      "default": "./index.js"
    },
    "./lit-element.js": {
      "types": "./lit-element.d.ts",
      "development": "./src/lit-element.js",
      "default": "./lit-element.js"
    },
    "./lit-element-renderer.js": {
      "types": "./lit-element-renderer.d.ts",
      "development": "./src/lit-element-renderer.js",
      "default": "./lit-element-renderer.js"
    },
    "./decorators.js": {
      "types": "./decorators.d.ts",
      "development": "./src/vendor/decorators.js",
      "default": "./decorators.js"
    },
    "./reactive-controller.js": {
      "types": "./reactive-controller.d.ts",
      "development": "./src/vendor/reactive-controller.js",
      "default": "./reactive-controller.js"
    },
    "./decorators/custom-element.js": {
      "types": "./decorators/custom-element.d.ts",
      "development": "./src/vendor/decorators/custom-element.js",
      "default": "./decorators/custom-element.js"
    },
    "./decorators/event-options.js": {
      "types": "./decorators/event-options.d.ts",
      "development": "./src/vendor/decorators/event-options.js",
      "default": "./decorators/event-options.js"
    },
    "./decorators/property.js": {
      "types": "./decorators/property.d.ts",
      "development": "./src/vendor/decorators/property.js",
      "default": "./decorators/property.js"
    },
    "./decorators/query-all.js": {
      "types": "./decorators/query-all.d.ts",
      "development": "./src/vendor/decorators/query-all.js",
      "default": "./decorators/query-all.js"
    },
    "./decorators/query-assigned-elements.js": {
      "types": "./decorators/query-assigned-elements.d.ts",
      "development": "./src/vendor/decorators/query-assigned-elements.js",
      "default": "./decorators/query-assigned-elements.js"
    },
    "./decorators/query-assigned-nodes.js": {
      "types": "./decorators/query-assigned-nodes.d.ts",
      "development": "./src/vendor/decorators/query-assigned-nodes.js",
      "default": "./decorators/query-assigned-nodes.js"
    },
    "./decorators/query-async.js": {
      "types": "./decorators/query-async.d.ts",
      "development": "./src/vendor/decorators/query-async.js",
      "default": "./decorators/query-async.js"
    },
    "./decorators/query.js": {
      "types": "./decorators/query.d.ts",
      "development": "./src/vendor/decorators/query.js",
      "default": "./decorators/query.js"
    },
    "./decorators/state.js": {
      "types": "./decorators/state.d.ts",
      "development": "./src/vendor/decorators/state.js",
      "default": "./decorators/state.js"
    }
  },
  "files": [
    "src",
    "vendor",
    "*.d.ts",
    "*.js",
    "README.MD"
  ],
  "repository": "https://github.com/popeindustries/lit.git",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@popeindustries/lit-html": "workspace:*",
    "@popeindustries/lit-html-server": "workspace:*"
  },
  "devDependencies": {
    "@lit/reactive-element": "^1.6.1",
    "lit-element": "^3.2.2"
  },
  "scripts": {
    "build": "node ./scripts/build.js"
  }
}
