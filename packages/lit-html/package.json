{
  "name": "@popeindustries/lit-html",
  "version": "5.0.0-beta.1",
  "description": "Enable hydration of lit-html templates rendered on the server with @popeindustries/lit-html-server",
  "author": "Alexander Pope <alex@pope.industries>",
  "keywords": [
    "html template",
    "lit-html",
    "ssr",
    "stream",
    "streaming",
    "tagged template literal",
    "template",
    "template render"
  ],
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./partial-hydration-mixin.js": "./partial-hydration-mixin.js",
    "./directives/*": "./directives/*"
  },
  "files": [
    "directives",
    "*.d.ts",
    "*.js",
    "README.MD"
  ],
  "repository": "https://github.com/popeindustries/lit.git",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "lit-html": "^2.2.7"
  },
  "devDependencies": {},
  "scripts": {
    "build": "node ./scripts/build.js",
    "clean": "git clean -x -f",
    "test": "pnpm run build && pnpm run lint && pnpm run test:browser",
    "test:browser": "node ./test/browser/cli.js"
  }
}
